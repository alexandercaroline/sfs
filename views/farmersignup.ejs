<body>


<div class="container">
       
    <div class="row">
        <div class="col-md-4 col-md-offset-4 text-center">
            <!-- <img src="assets/img/logo.png" alt=""/> -->
          <H1>SMART FARMERS' SYSTEM</H1>
          <% try {
              if(success != null){
              %>
                <div class="alert alert-success" role="alert">
                    Registration received! You will be able to login once we approve your request and alerted you via your emailError.
                </div>
              <%
             }
          } catch (error) {
            console.log(error);
          } %>
        </div>
        <div class="col-md-6 col-md-offset-4">
            <div class="login-panel panel panel-default">                  
                <div class="panel-heading">
                    <h3 class="panel-title">Please Sign up As a FARMER</h3>
                </div>
                <div class="panel-body">
                    <h4 class="text-center text-success text-bold">To Register first send 500ksh to Mpesa number 0702201445 then Include transaction number on this form</h4>
                    <form role="form" method="POST" action="/auth/farmersignup" >

                        <fieldset>
                            <div class="form-group">
                                <label for="E-mail" class="col-md-4 control-label">Email Address</label>
                                <div class="col-md-8">
                                    <input class="form-control" value="<%=model.email != undefined ? model.email: ''%>" placeholder="E-mail" name="email" type="email" autofocus>
                                    <span class="text-danger"><%=er.emailError != undefined ? er.emailError : ''%></span>
                                
                                </div>
                            
                            </div>
                           <br> </br>
                           
                            <div class="form-group">
                                <label for="First-name" class="col-md-4 control-label">First name</label>
                                <div class="col-md-8">
                                    <input class="form-control" value="<%=model.fName != undefined ? model.fName: ''%>" placeholder="First-name" name="fName" type="text"  autofocus >
                                    <span class="text-danger"><%=er.fNameError != undefined ? er.fNameError : ''%></span>
                                </div>
                            </div>
                        <br></br>

                        <div class="form-group">
                            <label for="Last-name" class="col-md-4 control-label">Last name</label>
                            <div class="col-md-8">
                                <input class="form-control" placeholder="Last name" value="<%=model.lName != undefined ? model.lName: ''%>" name="lName" type="text"  autofocus >
                                <span class="text-danger"><%=er.lNameError != undefined ? er.lNameError : ''%></span>
                            </div>
                        </div>
                        <br> </br>

                        <div class="form-group">
                            <label for="Phone-Number" class="col-md-4 control-label">Phone Number</label>
                            <div class="col-md-8">
                                <input class="form-control" value="<%=model.phone != undefined ? model.phone: ''%>" placeholder="Phone-Number" name="phone" type="number"  autofocus >
                                <span class="text-danger"><%=er.phoneError != undefined ? er.phoneError : ''%></span>
                            </div>
                        </div>
                        <br> </br>


                        <div class="form-group">
                            <label for="Location" class="col-md-4 control-label">Location</label>
                            <div class="col-md-8">
                                <input class="form-control" placeholder="location"  value="<%=model.location != undefined ? model.location: ''%>" name="location" type="text"  autofocus >
                                <span class="text-danger"><%=er.locationError != undefined ? er.locationError : ''%></span>
                            </div>
                        </div>
                        <br></br>

                        <div class="form-group">
                            <label for="County" class="col-md-4 control-label">County</label>
                            <div class="col-md-8">
                                <select name="county" class="form-control">
                                        <option value="Mombasa">Mombasa</option>
                                        <option value="Isiolo">Isiolo</option>
                                        <option value="Murang'a">Murang'a</option>
                                        <option value="Laikipia">Laikipia</option>
                                        <option value="Siaya">Siaya</option>
                                        <option value="Kwale">Kwale</option>
                                        <option value="Meru">Meru</option>
                                        <option value="Kiambu">Kiambu</option>
                                        <option value="Nakuru">Nakuru</option>
                                        <option value="Kisumu">Kisumu</option>
                                        <option value="Kilifi">Kilifi</option>
                                        <option value="Tharaka-Nithi">Tharaka-Nithi</option>
                                        <option value="Turkana">Turkana</option>
                                        <option value="Narok">Narok</option>
                                        <option value="Homa Bay">Homa Bay</option>
                                        <option value="Tana River">Tana River</option>
                                        <option value="Embu">Embu</option>
                                        <option value="West Pokot">West Pokot</option>
                                        <option value="Kajiado">Kajiado</option>
                                        <option value="Migori">Migori</option>
                                        <option value="Lamu">Lamu</option>
                                        <option value="Kitui">Kitui</option>
                                        <option value="Samburu">Samburu</option>
                                        <option value="Kericho">Kericho</option>
                                        <option value="Kisii">Kisii</option>
                                        <option value="Taita-Taveta">Taita-Taveta</option>
                                        <option value="Machakos">Machakos</option>
                                        <option value="Trans Nzoia">Trans Nzoia</option>
                                        <option value="Bomet">Bomet</option>
                                        <option value="Nyamira">Nyamira</option>
                                        <option value="Garissa">Garissa</option>
                                        <option value="Makueni">Makueni</option>
                                        <option value="Uasin Gishu">Uasin Gishu</option>
                                        <option value="Kakamega">Kakamega</option>
                                        <option value="Nairobi">Nairobi</option>
                                        <option value="Wajir">Wajir</option>
                                        <option value="Nyandarua	">Nyandarua	</option>
                                        <option value="Elgeyo-Marakwet">Elgeyo-Marakwet</option>
                                        <option value="Vihiga">Vihiga</option>
                                        <option value="Mandera">Mandera</option>
                                        <option value="Nyeri">Nyeri</option>
                                        <option value="Nandi">Nandi</option>
                                        <option value="Bungoma">Bungoma</option>
                                        <option value="Marsabit">Marsabit</option>
                                        <option value="Kirinyaga">Kirinyaga</option>
                                        <option value="Baringo">Baringo</option>
                                        <option value="Busia">Busia</option>
                                </select>
                            </div>
                        </div>
                     
                        
                        <br></br>
                        <div class="form-group">
                            <label for="Password" class="col-md-4 control-label">Password</label>
                            <div class="col-md-8">
                                <input class="form-control" value="<%=model.password != undefined ? model.password: ''%>" placeholder="Password" name="password" type="password" value="" autofocus >
                                <span class="text-danger"><%=er.passwordError != undefined ? er.passwordError : ''%></span>
                             </div>
                          </div>
                         <br></br>
                        
                         <div class="form-group">
                            <label for="confirm-Password" class="col-md-4 control-label">Confirm Password</label>
                       
                            <div class="col-md-8">
                                <input class="form-control" value="<%=model.conPassword != undefined ? model.conPassword: ''%>" placeholder="Password" name="conPassword" type="password" value="" autofocus >
                                <span class="text-danger"><%=er.conPasswordError != undefined ? er.conPasswordError : ''%></span>
                            </div>
                         </div>
                         
                         <br><br>
                         <div class="form-group mt-2">
                            <label for="confirm-Password" class="col-md-4 control-label">MPESA Transaction code :</label>
                       
                            <div class="col-md-8">
                                <input class="form-control" value="<%=model.mpesa != undefined ? model.mpesa : ''%>" placeholder="" name="mpesa" required autofocus >
                                <span class="text-danger"><%=er.mpesaError != undefined ? er.mpesaError : ''%></span>
                            </div>
                         </div>
                         <br><br>


                            <button class="btn btn-lg btn-success btn-block">Signup</button>

                            
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
